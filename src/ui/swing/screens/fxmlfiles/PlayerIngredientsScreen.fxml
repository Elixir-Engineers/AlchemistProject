<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="450.0" prefWidth="900.0" stylesheets="@../cssfiles/screenstyles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.swing.screens.screencontrollers.PlayerIngredientsScreenController">
    <children>
       

        <!-- Quit Button -->
        <Button fx:id="quitButton" layoutX="768.0" layoutY="21.0" mnemonicParsing="false" onAction="#handleQuitAction" style="-fx-font-size: 16; -fx-text-fill: white;" text="Back" />

        <!-- Ingredients Container -->
        <HBox fx:id="ingredientsContainer" layoutX="25" layoutY="20" prefHeight="408.0" prefWidth="730.0" spacing="20" />
    </children>
</AnchorPane>
